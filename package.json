{
  "name": "focus",
  "version": "2.2.0",
  "description": "Focus is a Bootstrap theme for creative people.",
  "author": "webvy",
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "scripts": {
    "clean": "del-cli dist",
    "purgecss": "purgecss -c purgecss.config.js -o dist/assets/css/",
    "build": "npm run clean && eleventy && npm run purgecss",
    "start": "eleventy --serve"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "autoprefixer": "^10.4.17",
    "del-cli": "^5.1.0",
    "eleventy-sass": "^2.2.4",
    "postcss": "^8.4.35",
    "purgecss": "^4.1.3"
  },
  "dependencies": {
    "@11ty/eleventy-img": "^4.0.2",
    "@midzer/tobii": "^2.5.0",
    "@quasibit/eleventy-plugin-schema": "^1.11.0",
    "bootstrap": "^5.3.2",
    "eleventy": "^1.0.6",
    "sharp": "^0.33.2"
  }
}
